<p>The current time is <%= @time %></p>
<h1>Sign up</h1>




<div class="row">
<div class="span8 offset1">
	
    <%= form_for(@user) do |f| %>
    <%= render 'users/error/messages' %>

      <%= f.label :name %>
      <%= f.text_field :name  %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.label :password_confirmation, "Confirmation" %>
      <%= f.password_field :password_confirmation %>

     <ul><%= f.submit "Create my account", class: "btn btn-large btn-primary " %></ul>
    <% end %>
    </div>
    </div>
    

  
  